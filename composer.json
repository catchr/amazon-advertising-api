{
  "name": "powersrc/amazon-advertising-api",
  "type": "library",
  "description": "Amazon Advertising API Client Library",
  "keywords": [
    "framework",
    "amazon",
    "advertising api",
    "client"
  ],
  "homepage": "https://powersrc.net/",
  "license": "MIT",
  "authors": [
    {
      "name": "Jason Hebert",
      "email": "jason.hebert@powersrc.net"
    },
    {
      "name": "Matthew Holder",
      "email": "matthew.holder@powersrc.net"
    }
  ],
  "autoload": {
    "psr-4": {
      "PowerSrc\\AmazonAdvertisingApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PowerSrc\\AmazonAdvertisingApi\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.1.3",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-zlib": "*",
    "guzzlehttp/guzzle": "^6.3",
    "psr/log": "^1.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "2.11",
    "phpunit/phpunit": "~7.0"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
